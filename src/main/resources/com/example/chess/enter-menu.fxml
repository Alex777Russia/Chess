<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.chess.Controllers.MenuController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button onMouseClicked="#startLocalGame" fx:id="localGameButton" mnemonicParsing="false" text="Local Game" />
                  <Button fx:id="networkGameButton" mnemonicParsing="false" text="Network Game" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
