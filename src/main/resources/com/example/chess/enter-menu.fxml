<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane prefHeight="406.0" prefWidth="715.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.chess.Controllers.MenuController">
<children>
   <ImageView fitHeight="429.0" fitWidth="730.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@../../../images/menu.gif" />
      </image></ImageView>
   <AnchorPane prefHeight="406.0" prefWidth="726.0">
      <children>
         <RadioButton onMouseClicked="#RadioButtonSwitch" layoutX="620.0" layoutY="30.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="87.0" text="  Volume" textAlignment="CENTER" fx:id="volumeButton">
            <font>
               <Font name="Times New Roman" size="15.0" />
            </font></RadioButton>
         <Button layoutX="315.0" layoutY="335.0" mnemonicParsing="false" onMouseClicked="#startLocalGame" prefHeight="29.0" prefWidth="105.0" text="Start match" textAlignment="CENTER">
            <font>
               <Font name="Times New Roman" size="15.0" />
            </font></Button>
      </children>
   </AnchorPane>
</children>
</StackPane>
